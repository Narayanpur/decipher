<style name="respview.client.js" with="Qpercentage" mode="after" wrap="ready">

<![CDATA[
setTimeout(function() {
  $ ('.qlayout_container').append('<div class="percentage-box" style="position: absolute; top: 65px; left: 770px; font-size: 70px;"><div class="percentage" style="display: inline;">0</div>%</div>')

  var totalSum = 0,
        inputs = $ ('.survey-q-answers input')

  setInterval(function() {
    totalSum = 0

    for (var i = inputs.length - 1; i >= 0; i--) {
      totalSum += parseInt(inputs.eq(i).val())
    };

    $ ('.percentage').html((totalSum - 1) || 0)

  if ($ ('#btn_continue').attr('disabled')) {
    $ ('#btn_continue').css('display','none')
  
  }
  if ($ ('.qwidget_button_wrapper .qwidget_background').css('background-position') == "100% 50%"){
    $ ('#btn_continue').css('display','block') 
    $ ('#btn_continue').removeAttr('disabled')
  }
  else{
   $ ('#btn_continue').css('display','block')
  };

  }, 10);
}, 150);
]]>

</style>



