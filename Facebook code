d = gv.request.device

if d.isMobileDevice():  # True if device is any mobile device
  hDevice.val = 0
elif d.isSmartphone():  # True if smartphone category
  hDevice.val = 0
elif d.isTablet():  # True if tablet category
  hDevice.val = 1
elif d.desktop:
  hDevice.val = 2
</exec>

